<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>demoapp</display-name>
  
	
	<servlet>
		<servlet-name>abc</servlet-name>
		<servlet-class>com.demoapp.AddServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>abc</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>vf</servlet-name>
		<servlet-class>com.demoapp.verifyServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>vf</servlet-name>
		<url-pattern>/verify</url-pattern>
	</servlet-mapping>
	
	
	
	
	<servlet>
		<servlet-name>duo</servlet-name>
		<servlet-class>com.demoapp.DUOauth</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>duo</servlet-name>
		<url-pattern>/duo</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>welcome</servlet-name>
		<servlet-class>com.demoapp.welcomeServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>welcome</servlet-name>
		<url-pattern>/welcome</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>com.demoapp.logoutServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	

	
	
	

	
</web-app>